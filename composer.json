{
  "name": "idlab/helper-bundle",
  "description": "Idlab helpers",
  "type": "symfony-bundle",
  "version": "0.0.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Vetterli",
      "email": "michael@idlab.ch"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/config": "^5.4|^6.0",
    "symfony/dependency-injection": "^5.4|^6.0",
    "symfony/http-kernel": "^5.4|^6.0",
    "phpmyadmin/sql-parser": "^5.5"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.1",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Idlab\\HelperBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Idlab\\HelperBundle\\Tests\\": "tests/"
    }
  }
}
